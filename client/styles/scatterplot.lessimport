div.scatterplot-container {
	padding: 0.5em 0.5em 0 0.5em;
	background-color: @color_beige;
}

path.circle {
	cursor: pointer;
}

.scatterplot{
	text {
		fill: @color_pantheon_black !important;
	}
	.xaxis line{
		stroke: @color_pantheon_black !important;
	}
	.yaxis line{
		stroke: @color_pantheon_black !important;
	}
	.chart{
		rect#background{
			fill: @color_beige;
			stroke: @color_pantheon_black;
		}
	}
}